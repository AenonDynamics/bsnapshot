{
    "name": "bsnapshot",
    "version": "0.2.0",
    "package-monster": "0.4.0",
    "description": "btrfs based backup tool with daily/weekly/monthly/yearly schedules",
    "license": "MPL-2.0",
    "author": "Andi Dittrich <opensource@aenon-dynamics.com>",
    "website": "https://github.com/AenonDynamics/bsnapshot",
    "bugs": "https://github.com/AenonDynamics/bsnapshot/issues",
    "multiversion": false,

    "debian": {

        "section": "admin",
        "priority": "optional",

        "dependencies": [
            "tree",
            "btrfs-progs"
        ],

        "conf": [
        ],

        "etc": [
            {
                "src": "config/bsnapshot.conf",
                "dst": "bsnapshot.conf"
            }
        ],

        "bin": [
        ],

        "sbin": [
            "bsnapshot"
        ]
    }
}
